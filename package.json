{
  "name": "strategos",
  "version": "1.0.0",
  "description": "Web dashboard for orchestrating AI coding agents in tmux sessions",
  "private": true,
  "workspaces": [
    "server"
  ],
  "scripts": {
    "start": "cd server && npm start",
    "server": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --headed"
  },
  "bin": {
    "strategos": "./bin/strategos"
  },
  "keywords": [
    "claude-code",
    "gemini-cli",
    "orchestrator",
    "tmux",
    "ai-agents",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "concurrently": "^8.2.2"
  }
}
